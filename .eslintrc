{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "React": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": ["react"],
  "rules": {
    "quotes": "single",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
